{
  "manifest_version": 2,

  "name": "Luoo",
  "description": "This extension help luooer download his favourite songs!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Luoo"
  },
  "permissions": [
    "cookies",
    "http://*.luoo.net/*"
  ]
}
