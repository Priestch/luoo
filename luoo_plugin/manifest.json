{
    "manifest_version": 2,

    "name": "Luoo",
    "description": "Help to download favourite songs from www.luoo.net",
    "version": "0.0.2.0",

    "options_page": "options.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Luoo"
    },
    "permissions": [
        "downloads",
        "http://*.luoo.net/*",
        "http://priestch.com/*"
    ]
}
